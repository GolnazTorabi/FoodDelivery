<resources>
    <!-- Base application theme. -->
    <style name="Theme.DinDinTest" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryDark">@color/purple_700</item>
        <item name="colorAccent">@color/colorblackDay</item>

        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.DinDinTest.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.DinDinTest.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.DinDinTest.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Custom" parent="@android:style/TextAppearance.Large">
        <item name="android:typeface">monospace</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="tabSelectedTextColor">@color/black</item>
        <item name="tabTextColor">@color/gray</item>
    </style>
</resources>